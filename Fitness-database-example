-- Table: Exercise
CREATE TABLE [dbo].[Exercise] (
    [Exercise_id] [int] NOT NULL,
      NOT NULL,
    [Difficulty_id] [int] NOT NULL,
   ARY KEY CLUSTERED ([Exercise_id] ASC),
    CONSTRAINT [FK_Exercise_ExerciseDifficulty_Difficulty_id] FOREIGN KEY ([Difficulty_id]) REFERENCES [dbo].[ExerciseDifficulty] ([Difficulty_id])
);

-- Table: ExerciseBody
CREATE TABLE [dbo].[ExerciseBody] (
    [Exercise_id] [int] NOT NULL,
    [Body_id] [int] NOT NULL,
    [Impact_level] [nvarchar](max) NOT NULL,
    CONSTRAINT [PK_ExerciseBody] PRIMARY KEY CLUSTERED ([Exercise_id] ASC, [Body_id] ASC),
    CONSTRAINT [FK_ExerciseBody_Body_Body_id] FOREIGN KEY ([Body_id]) REFERENCES [dbo].[Body] ([Body_id]),
    CONSTRAINT [FK_ExerciseBody_Exercise_Exercise_id] FOREIGN KEY ([Exercise_id]) REFERENCES [dbo].[Exercise] ([Exercise_id])
);

